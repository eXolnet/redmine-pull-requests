<div class="contextual">
<%= link_to l(:button_edit), edit_pull_path(@pull), :onclick => 'showAndScrollTo("update", "issue_notes"); return false;', :class => 'icon icon-edit', :accesskey => accesskey(:edit) if @pull.editable? %>
<%= watcher_link(@pull, User.current) %>
<%= link_to l(:button_delete), issue_path(@pull), :data => {:confirm => pulls_destroy_confirmation_message(@pull)}, :method => :delete, :class => 'icon icon-del' if @pull.deletable? %>
</div>
